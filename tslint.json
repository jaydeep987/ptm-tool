{
  "extends": ["tslint:latest", "tslint-react"],
  "rules": {
    "quotemark": [true, "single", "jsx-double"],
    "jsx-wrap-multiline": false,
    "no-submodule-imports": [false],
    "no-unused-expression": [true],
    "max-line-length": [true, 180],
    "object-literal-sort-keys": [false],
    "no-console": [false]
  }
}